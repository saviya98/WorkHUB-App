<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Worker_Profile">

  <include
      layout="@layout/signuponworkhub" />

  <FrameLayout
      android:id="@+id/container"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
     />



  <com.google.android.material.bottomnavigation.BottomNavigationView
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:id="@+id/bottom_wnav"
      android:layout_alignParentBottom="true"
      android:background="@color/custompurple"
      app:itemTextColor="@color/customwhite"
      app:itemIconTint="@color/customwhite"
      app:menu="@menu/bottom_wmenu"

      />




</RelativeLayout>